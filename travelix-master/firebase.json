{
    "hosting": {
        "public": "public",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [{
                "source": "/login",
                "destination": "./login.html"

            },
            {
                "source": "/about",
                "destination": "./about.html"

            },
            {
                "source": "/blog",
                "destination": "./blog.html"

            },
            {
                "source": "/contact",
                "destination": "./contact.html"

            },
            {
                "source": "/home",
                "destination": "./home.html"

            },

            {
                "source": "/car_rentals",
                "destination": "./car_rentals.html"

            },
            {
                "source": "/hotels",
                "destination": "./hotels.html"

            },
            {
                "source": "/trips",
                "destination": "./trips.html"

            },
            {
                "source": "/single_listing",
                "destination": "./single_listing.html"

            }

        ]
    }
}